ssh -p 11111 root@192.168.0.218 <<EOF
    cd ~/ovni
    git pull origin master
    curl -o-   https://raw.githubusercontent.com/creationix/nvm/v0.33.8/install.sh    | bash
    . ~/.nvm/nvm.sh
    nvm install 17.4.0
    npm install
    npm install -g pm2
    pm2 restart ecosystem.config.js
    exit
EOF